<%= form_with scope: :address, url: addresses_path  ,local: true do |form| %>
  <div class="form-group">
    <%= form.text_field :add , placeholder:'Enter address',class:"form-control", required: true %>
  </div>
  <div class="form-group">
    <%= form.text_field :words , placeholder:'Enter words', class:"form-control", required: true %>
  </div>
   <div class="form-group">
     <%= form.text_field :lat , placeholder:'Enter lat', class:"form-control", required: true  %>
  </div>
   <div class="form-group">
     <%= form.text_field :lng , placeholder:'Enter lng', class:"form-control", required: true  %>
  </div>
  <%= form.submit 'Submit' ,class:"btn btn-outline-primary"%>
  <%= link_to 'Back',  welcome_index_path, class:"btn btn-outline-primary"%>
<% end %>
